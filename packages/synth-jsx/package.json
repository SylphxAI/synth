{
	"name": "@sylphx/synth-jsx",
	"version": "0.2.3",
	"description": "JSX/TSX parser using Synth's universal AST",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"src",
		"README.md"
	],
	"scripts": {
		"test": "bun test",
		"typecheck": "tsc --noEmit",
		"build": "bunup",
		"prepack": "bun run build"
	},
	"keywords": [
		"jsx",
		"tsx",
		"react",
		"parser",
		"ast",
		"synth"
	],
	"author": "Synth",
	"license": "MIT",
	"dependencies": {
		"@sylphx/synth": "workspace:*",
		"acorn": "^8.12.1",
		"acorn-jsx": "^5.3.2"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"typescript": "^5.6.0"
	}
}
