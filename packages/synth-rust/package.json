{
  "name": "@sylphx/synth-rust",
  "version": "0.1.0",
  "description": "Rust parser using Synth's universal AST",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
