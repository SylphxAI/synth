{
	"name": "@sylphx/synth-docs",
	"version": "0.1.3",
	"description": "Documentation generator for Synth AST - extract docs from any language",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"src",
		"README.md"
	],
	"scripts": {
		"test": "bun test",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"docs",
		"documentation",
		"generator",
		"ast",
		"synth",
		"jsdoc",
		"markdown"
	],
	"author": "Synth",
	"license": "MIT",
	"dependencies": {
		"@sylphx/synth": "workspace:*"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"typescript": "^5.6.0"
	}
}
