{
	"name": "@sylphx/synth",
	"version": "0.3.1",
	"description": "Synth - The world's fastest AST processor with zero dependencies",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"dev": "bun build src/index.ts --outdir dist --target node --format esm --watch",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"build": "bunup",
		"prepack": "bun run build"
	},
	"keywords": [
		"synth",
		"ast",
		"parser",
		"tree",
		"types",
		"query",
		"traverse",
		"zipper",
		"incremental",
		"plugin",
		"optimization",
		"fast",
		"performance"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.0.0",
		"typescript": "^5.6.0"
	},
	"files": [
		"dist",
		"src",
		"README.md"
	],
	"publishConfig": {
		"access": "public"
	}
}
