{
  "name": "@sylphx/synth-msgpack",
  "version": "0.1.0",
  "description": "MessagePack parser using Synth's universal AST",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "msgpack",
    "messagepack",
    "binary",
    "serialization",
    "parser",
    "ast",
    "synth"
  ],
  "author": "Synth",
  "license": "MIT",
  "dependencies": {
    "@sylphx/synth": "workspace:^",
    "@msgpack/msgpack": "^3.0.0-beta2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
