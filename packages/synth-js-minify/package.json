{
  "name": "@sylphx/synth-js-minify",
  "version": "0.1.0",
  "description": "JavaScript minifier using Synth's universal AST",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "keywords": [
    "minifier",
    "javascript",
    "ast",
    "synth",
    "compress",
    "uglify"
  ],
  "author": "SylphX",
  "license": "MIT",
  "dependencies": {
    "@sylphx/synth": "workspace:^",
    "@sylphx/synth-js": "workspace:^"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
