{
  "name": "@sylphx/synth-toml",
  "version": "0.1.0",
  "description": "TOML parser using Synth's universal AST - hand-written, zero dependencies",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "keywords": [
    "toml",
    "parser",
    "ast",
    "synth",
    "config",
    "configuration",
    "hand-written"
  ],
  "author": "SylphX",
  "license": "MIT",
  "dependencies": {
    "@sylphx/synth": "workspace:^"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
